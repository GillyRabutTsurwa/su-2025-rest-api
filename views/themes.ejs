<%- include ("./partials/header") %>
<div style="margin-left: auto;">
    <% if (user) { %>
        User: <h4> <%= user.username %> </h4>
        <a href="/logout">Logout</a>
        <a href="/create-theme">Add Theme</a>
    <% } %>
</div>
    <div class="plugins-container">
        <% themes.forEach((currentTheme)=> { %>
            <a href="/themes/<%= currentTheme.codebaseName %>">
                <p class="joke">
                    <%= currentTheme.name %>
                </p>
            </a>
        <% }) %>
    </div>
