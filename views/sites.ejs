<%- include ("./partials/header") %>
<div style="margin-left: auto;">
    <% if (user) { %>
        User: <h4> <%= user.username %> </h4>
        <a href="/logout">Logout</a>
        <a href="/create-site">Add Sites</a>
    <% } %>
</div>
    <div class="sitess-container">
        <% sites.forEach((currentSites)=> { %>
            <a href="/sitess/<%= currentSites.codebaseName %>">
                <p class="joke">
                    <%= currentSites.name %>
                </p>
            </a>
        <% }) %>
    </div>
