<%- include ("./partials/header") %>
<div style="margin-left: auto;">
    <% if (user) { %>
        User: <h4> <%= user.username %> </h4>
        <a href="/logout">Logout</a>
        <a href="/create-site">Add Sites</a>
    <% } %>
</div>
    <div class="sites-container">
        <% sites.forEach((currentSites)=> { %>
            <a href="/sites/<%= currentSites.name %>" class="joke">
                <%= currentSites.name %>
            </a>
        <% }) %>
    </div>
