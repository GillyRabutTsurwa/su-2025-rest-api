{
    "name": "su-2025-rest-api",
    "version": "1.0.0",
    "description": "REST API for SU 2025 Project",
    "main": "app.js",
    "scripts": {
        "build": "rimraf dist && tsc && npm run copy-assets",
        "prestart": "npm run build",
        "start": "NODE_ENV=production node dist/app.js",
        "copy-assets": "ts-node tools/copyAssets.ts",
        "preserve": "npm run build",
        "serve": "tsc -w & NODE_ENV=development nodemon dist/app.js",
        "preview": "tsc -w & NODE_ENV=production nodemon dist/app.js"
    },
    "repository": {
        "type": "git",
        "url": "coming-soon"
    },
    "keywords": [
        "rest-api",
        "express",
        "mongodb",
        "node"
    ],
    "author": "Gilbert Rabut Tsurwa",
    "license": "ISC",
    "dependencies": {
        "body-parser": "^1.20.2",
        "dotenv": "^16.3.1",
        "ejs": "^3.1.9",
        "express": "^4.18.2",
        "express-session": "^1.17.3",
        "mongoose": "^7.3.0",
        "octokit": "^3.0.0",
        "passport": "^0.6.0",
        "passport-local": "^1.0.0",
        "passport-local-mongoose": "^8.0.0",
        "rimraf": "^5.0.1",
        "shelljs": "^0.8.5"
    },
    "engines": {
        "node": ">=14.20.1"
    },
    "devDependencies": {
        "@octokit/types": "^11.1.0",
        "@types/body-parser": "^1.19.2",
        "@types/express": "^4.17.17",
        "@types/express-session": "^1.17.7",
        "@types/mongoose": "^5.11.97",
        "@types/passport": "^1.0.12",
        "@types/passport-local": "^1.0.35",
        "@types/passport-local-mongoose": "^6.1.1",
        "@types/shelljs": "^0.8.12"
    }
}
